{
  "Elasticsearch": {
    "Url": "http://localhost:9200",
    "Index": "flytit-chatbot",
    "DeleteIndexFirst": false
  },
  "Indexer": {
    "RunOnStartup": true,
    "Folder": "", //Mappen for evt filer som skal hentes
    "Pattern": "*.pdf;*.docx;*.html;*.txt;*.md",
    "Recursive": true,
    "Site": "intranett",
    "MaxConcurrency": 2,
    "RenderPages": "true",
    "ImageCaptions": "true",
    "CaptionMode": "auto"
  },
  "Crawler": {
    "RunOnStartup": true,
    "StartUrl": "", //Her skrives URL for nettsiden som skal brukes 
    "MaxPages": 300
  },
  "OpenAI": {
    "ApiKey": "<sett miljøvariabel eller legg nøkkel her>",
    "EmbeddingModel": "text-embedding-3-small",
    "ChatModel": "gpt-4o-mini"
  },
  "Cors": {
    "AllowedOrigins": [ "http://localhost:5173" ]
  },
  "Urls": "http://0.0.0.0:5000"
}

